{
  "name": "gulp-svg2png",
  "version": "1.0.0",
  "description": "A gulp plugin for converting SVGs to PNGs.",
  "author": "André König <andre.koenig@posteo.de>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "tsd": "tsd reinstall -so",
    "compile": "tsc",
    "build": "npm i && npm run tsd && npm run compile",
    "mocha": "mocha ./specs/*.spec.js",
    "test": "npm run build && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:akoenig/gulp-svg2png.git"
  },
  "keywords": [
    "gulpplugin",
    "svg",
    "png",
    "converter",
    "svg2png"
  ],
  "bugs": {
    "url": "https://github.com/akoenig/gulp-svg2png/issues"
  },
  "homepage": "https://github.com/akoenig/gulp-svg2png",
  "dependencies": {
    "gulp-util": "^3.0.5",
    "map-stream": "0.0.6",
    "map-stream-limit": "^1.1.0",
    "svg2png": "^1.1.1"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "imagesize": "^1.0.0",
    "mocha": "^2.2.5",
    "tsd": "^0.6.5",
    "typescript": "^1.7.3"
  }
}